				"description": "The endpoint to validate users credentials and generate an access token.",
				"operationId": "AccessTokenPost",
				"consumes": 
				[
					
				],

				"produces": 
				[
					"application/json",
					"text/json"
				],

				"parameters": 
				[
					{
						"name": "username",
						"in": "query",
						"description": "The username.",
						"required": true,
						"type": "string"
					},

					{
						"name": "password",
						"in": "query",
						"description": "The password.",
						"required": false,
						"type": "string"
					}
				],

				"responses": 
				{
					"200": 
					{
						"description": "OK",
						"schema": 
						{
							"$ref": "#/definitions/TokenResult"
						}
					},

					"0": 
					{
						"description": "Unexpected error"
					}
				}
			}
		},

		"/accessToken/refresh": 
		{
			"post": 
			{
				"tags": 
				[
					"AccessTokenApi"
				],

				"summary": "Refresh an access token.",
				"description": "The endpoint to refresh an access token.",
				"operationId": "Refresh",
				"consumes": 
				[
					
				],

				"produces": 
				[
					"application/json",
					"text/json"
				],

				"responses": 
				{
					"200": 
					{
						"description": "OK",
						"schema": 
						{
							"$ref": "#/definitions/TokenResult"
						}
					},

					"0": 
					{
						"description": "Unexpected error"
					}
				}
			}
		}
	},

	"definitions": 
	{
		"TokenResult": 
		{
			"description": "",
			"type": "object",
			"properties": 
			{
				"accessToken": 
				{
					"description": "The generated authentication token.",
					"type": "string"
				},

				"expireDateTime": 
