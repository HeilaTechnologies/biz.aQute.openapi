{
  "swagger": "2.0",
  "info": {
    "version": "v1",
    "title": "imagereturn"
  },
  "basePath": "/v1",
  "paths": {
    "/image_multiple_mime": {
      "get": {
        "produces": [
          "image/png", "image/jpeg"
        ],
        "operationId": "image_multiple_mime",
        "responses": {
          "200": {
              "description":"",
                "schema": {
                    "type": "string",
                    "format":"binary"    
                }
          }
        }
      }
   },
  "/image_one_mime": {
      "get": {
        "produces": [
          "image/png"
        ],
        "operationId": "image_one_mime",
        "responses": {
          "200": {
              "description":"",
                "schema": {
                    "type": "string",
                    "format":"binary"    
                }
          }
        }
      }
  },
  "/image_json": {
      "get": {
        "produces": [
          "application/json"
        ],
        "operationId": "image_json",
        "responses": {
          "200": {
              "description":"",
                "schema": {
                    "type": "string",
                    "format":"binary"    
                }
          }
        }
      }
    },
  "/product/icon/{productTagId}/{iconType}/{size}": {
	  "get": {
	    "tags": [
	      "Product"
	    ],
	    "summary": "Gets the product icon.",
	    "operationId": "GetProductIcon",
	    "consumes": [],
	    "produces": [
	      "image/png",
	      "image/svg"
	    ],
	    "parameters": [
	      {
	        "name": "productTagId",
	        "in": "path",
	        "description": "The product tag identifier.",
	        "required": true,
	        "type": "string"
	      },
	      {
	        "name": "iconType",
	        "in": "path",
	        "description": "Type of the icon.(neutral,alarm,warning,comerror,notactive,ok)",
	        "required": true,
	        "type": "string",
	        "enum": [
	          "Neutral"
	        ]
	      },
	      {
	        "name": "size",
	        "in": "path",
	        "description": "The size.(24, 150)",
	        "required": true,
	        "type": "integer",
	        "format": "int32"
	      },
	      {
	        "name": "fileFormat",
	        "in": "query",
	        "description": "The file format.(png,svg)",
	        "required": false,
	        "type": "string",
	        "enum": [
	          "Png",
	          "Svg"
	        ]
	      }
	    ],
	    "responses": {
	      "200": {
	        "description": "icon image file of specified file format"
	      },
	      "404": {
	        "description": "Resource not found or no authorization"
	      }
	    }
	  }
	} 
    
  }
}
