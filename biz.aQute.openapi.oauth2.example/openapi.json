{
  "swagger":"2.0",
  "info":{
  	"title":"OAuth2 Example",
  	"version":"1.0"
  },
  "securityDefinitions":{
    "google":{
      "type":"oauth2",
      "flow":"accessCode",
      "scopes":{
        "openid":"",
        "email":""
      },
      "authorizationUrl":"",
      "tokenUrl":""
    }
  },
  "security":[
    {
      "google":[

      ]
    }
  ],
  "basePath":"/openapi/security/google",
  "paths":{
    "/authenticated/{action}":{
      "get":{
        "operationId":"authenticated",
        "parameters":[
          {
            "name":"action",
            "in":"path",
            "required":true,
            "type":"string"
          }
        ],
        "responses":{
          "200":{
            "schema":{
              "type":"boolean"
            }
          }
        }
      }
    },
    "/unauthenticated/{action}":{
      "get":{
        "operationId":"unauthenticated",
        "security":[

        ],
        "parameters":[
          {
            "name":"action",
            "in":"path",
            "required":true,
            "type":"string"
          }
        ],
        "responses":{
          "200":{
            "schema":{
              "type":"boolean"
            }
          }
        }
      }
    }
  }
}
